menu "Select libisp configuration options"
depends on PACKAGE_libAWIspApi

choice
	prompt "Select compile libisp to static or dynamic library"
	depends on PACKAGE_libAWIspApi
	default libisp_compile_static_lib

config libisp_compile_static_lib
	bool "compile static lib"
	help
	  compile static libs for libisp.

config libisp_compile_dynamic_lib
	bool "compile dynamic lib"
	help
	  compile dynamic lib for libisp.

endchoice

config libisp_build_libispapi
	bool "libisp build and install libawispapi for 3rd apps"
	depends on PACKAGE_libAWIspApi
	default n
	help
	  libisp build and install libawispapi for 3rd apps

config libisp_build_tunning_app
	bool "libisp build awTuningApps (will not install to firmware)"
	depends on PACKAGE_libAWIspApi
	default n
	help
	  libisp build awTuningApp

config libisp_support_fastboot
	bool "libisp support fastboot"
	depends on PACKAGE_libAWIspApi
	default n
	help
	  libisp support fastboot.

config libisp_software_tdm
